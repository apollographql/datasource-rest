  "dependencies": {
    "@apollo/utils.fetcher": "^3.0.0",
    "@apollo/utils.keyvaluecache": "^3.0.0",
    "@apollo/utils.logger": "^3.0.0",
    "@apollo/utils.withrequired": "^3.0.0",
    "@types/http-cache-semantics": "^4.0.1",
    "http-cache-semantics": "^4.1.1",
    "lodash.isplainobject": "^4.0.6",
    "node-fetch": "^2.6.7"
  },
  "peerDependencies": {
    "graphql": "^16.5.0"
  },
  "volta": {
    "node": "18.16.0",
    "npm": "9.6.7"
  }
